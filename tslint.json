{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "no-console": ["log"],
    "max-line-length": ["error",180],
    "no-empty-interface": false,
    "no-trailing-spaces": false,
    "no-trailing-whitespace": false,
    "no-empty": false,
    "ordered-imports": false,
    "object-literal-sort-keys": false
  },
  "rulesDirectory": []
}